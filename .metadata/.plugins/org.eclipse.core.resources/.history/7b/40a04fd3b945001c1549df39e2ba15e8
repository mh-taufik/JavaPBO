package Kasus_2;

import java.util.*;

abstract class SuperHero{
	private int powerLevel;
	private String name;
	private List<Power> powerList;
	
	public SuperHero(int heroLevel, String heroName) {
		this.powerLevel = heroLevel;
		this.name = heroName;
	}

	public String getName() {
		return name;
	}
	
	public int getPowerLevel() {
		return powerLevel;
	}
	
	abstract void identity();
	abstract void addPower(Power power);
	abstract void showPower();
}
